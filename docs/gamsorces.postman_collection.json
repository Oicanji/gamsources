	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"let host = pm.collectionVariables.get(\"host\");  ",
					"let username = pm.collectionVariables.get(\"username\");  ",
					"let password = pm.collectionVariables.get(\"password\");  ",
					"  ",
					"let body = JSON.stringify({  ",
					"\t\"user\": username,  ",
					"\t\"password\": password  ",
					"})  ",
					" ",
					"const postRequest = {  ",
					"\turl: host + '/user/login',  ",
					"\tmethod: 'POST',  ",
					"\ttimeout: 0,  ",
					"\theader: {  ",
					"\t\t\"Content-Type\": \"application/json\",",
					"\t\t\"Accept\": \"*/*\",",
					"\t},  ",
					"\tbody: body",
					"};  ",
					"",
					"pm.sendRequest(postRequest, function (err, res) {  ",
					"\tconsole.log(\"Login Response:\",  res);  ",
					"\tif (err) console.log(\"Error:\", err)  ",
					"\t\tlet response = res.json()",
					"        let authorization = response.access_token;",
					"\t\tpm.collectionVariables.set('authorization_token', authorization);",
					"    });"
				]
			}
		},
